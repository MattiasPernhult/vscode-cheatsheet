{
  "name": "vscode-cheatsheet",
  "displayName": "vscode-cheatsheet",
  "description": "Looking up code documentation for you",
  "version": "0.1.1",
  "publisher": "MattiasPernhult",
  "galleryBanner": {
    "color": "#19AEE0",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MattiasPernhult/vscode-cheatsheet.git"
  },
  "homepage": "https://github.com/MattiasPernhult/vscode-cheatsheet#readme",
  "bugs": {
    "url": "https://github.com/MattiasPernhult/vscode-todo/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.cheatsheet"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.cheatsheet",
        "title": "Cheatsheet: Find"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
